{
  "name": "weaki",
  "version": "1.0.0",
  "description": "An electron based tool for agile software documentation",
  "main": "build/app.js",
  "scripts": {
    "package": "mkdir dist -p && electron-packager . --out=dist --platform win32,linux,mas,darwin --arch=all --osx-sign && for i in dist/*/; do zip -rq \"${i%/}.zip\" \"$i\"; done ",
    "build": "gulp build",
    "docs": "jsdoc -c jsdoc.conf.json -d docs -t ./node_modules/ink-docstrap/template -r app.js src",
    "clean": "rm -rf dist build docs",
    "start": "electron build/app.js",
    "watch": "gulp watch",
    "test": "gulp test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fdiogo/weaki.git"
  },
  "keywords": [
    "weaki",
    "wiki",
    "electron",
    "agile",
    "documentation"
  ],
  "author": "Diogo Ferreira",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fdiogo/weaki/issues"
  },
  "homepage": "https://github.com/fdiogo/weaki#readme",
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "bluebird": "^3.5.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "electron": "^1.4.15",
    "electron-packager": "^8.1.0",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-standard": "^2.0.1",
    "github": "^9.0.0",
    "gulp": "^3.9.1",
    "gulp-add-src": "^0.2.0",
    "gulp-babel": "^6.1.2",
    "gulp-mocha": "^4.0.1",
    "gulp-pug": "^3.2.0",
    "gulp-sass": "^3.1.0",
    "gulp-watch": "^4.3.11",
    "ink-docstrap": "^1.3.0",
    "jsdoc": "^3.4.3",
    "jsdom": "^9.11.0",
    "mocha": "^3.1.2",
    "mocha-jsdom": "^1.1.0",
    "react-addons-test-utils": "^15.4.2",
    "rimraf": "^2.6.1"
  },
  "dependencies": {
    "draft-js": "^0.10.0",
    "highlight.js": "^9.9.0",
    "history": "^4.6.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router-dom": "^4.0.0",
    "simple-git": "^1.67.0"
  }
}
